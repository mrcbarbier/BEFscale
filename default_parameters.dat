        #Competition
        'competition': {
            'mean': 0.1,
            'multiscale': 0, #Switch ON/OFF multiscale
            'trait_exp':1, #Exponent for how spatial range of interaction changes with trait value
        },

        #Mortality
        'mortality': {
            'distribution': 'uniform',
            'range': (0.001, 0.01),
        },

